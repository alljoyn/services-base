[project]
	description = Base Services
[access]
        inheritFrom = All-Projects
[access "refs/heads/*"]
	create = group ldap/gerrit_baseservices_committers
	forgeAuthor = group ldap/gerrit_baseservices_committers
	push = group ldap/gerrit_baseservices_committers
	label-Code-Review = -2..+2 group ldap/gerrit_baseservices_committers
	submit = group ldap/gerrit_baseservices_committers
	editTopicName = group ldap/gerrit_baseservices_committers
	abandon = group ldap/gerrit_baseservices_committers
[access "refs/meta/config"]
	read = group ldap/gerrit_baseservices_committers
[access "refs/tags/*"]
	pushTag = group ldap/gerrit_baseservices_committers
	pushSignedTag = group ldap/gerrit_baseservices_committers
[access "refs/for/*"]
	pushMerge = group ldap/gerrit_baseservices_committers
[access "refs/for/refs/heads/feature/*"]
	pushMerge = group Non-Interactive Users
	pushMerge = group ldap/gerrit_baseservices_committers
